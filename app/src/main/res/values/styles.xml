<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- Support library compatibility -->
        <item name="actionBarStyle">@style/MyActionBar</item>
    </style>
    
    
    <style name="MyActionBar"
        parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/sickday_blue</item>
        <item name="android:title">null</item>
        <!-- Support library compatibility -->
        <item name="background">@color/sickday_blue</item>
    </style>

    <color name="sickday_blue">#5D8DB5</color>
    <color name="sickday_red">#A32626</color>
    <color name="facebook_blue">#31488B</color>
    <color name="font_color_blue">#266092</color>

</resources>
